<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TestYourStrenth">
        <activity android:name="com.example.testyourstrenth.QuizActivity" />
        <activity android:name="com.example.testyourstrenth.ResultActivity" />
        <activity android:name="com.example.testyourstrenth.AboutActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignupActivity" />

        <activity
            android:name=".LeaderboardActivity"
            android:exported="false"
            android:label="Leaderboard"
            android:theme="@style/Theme.TestYourStrenth" />


        <activity
            android:name="com.example.testyourstrenth.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>